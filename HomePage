@State var counter = 0
    var body: some View {
        ZStack{
            
            //ProgressView(value: 1)
            Image("OutlineBackGround").ignoresSafeArea()
           
            VStack{
                
                Text(String(counter))
                    .font(.largeTitle)
                    
                Spacer().frame(height:150)
                Button(action: {
                    counter += 1
                }, label: {
                    Image(systemName: "circle")
                        
                })
                
            }
            
        }
        
        
    }
